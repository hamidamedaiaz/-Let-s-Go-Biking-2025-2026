<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<system.serviceModel>

		<services>
			<service name="ProxyCacheService.ProxyCacheServiceImpl"
					 behaviorConfiguration="ProxyCacheServiceBehavior">

				<endpoint
					address=""
					binding="basicHttpBinding"
					contract="ProxyCacheService.IProxyCacheService" />

				<endpoint
					address="mex"
					binding="mexHttpBinding"
					contract="IMetadataExchange" />

			</service>
		</services>

		<behaviors>
			<serviceBehaviors>
				<behavior name="ProxyCacheServiceBehavior">
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>

	</system.serviceModel>

</configuration>
